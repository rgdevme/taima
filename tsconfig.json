{
	"compilerOptions": {
		"jsx": "react-jsx",
		"jsxImportSource": "@emotion/react",
		"target": "ES6",
		"module": "ES6",
		"allowSyntheticDefaultImports": true,
		"moduleResolution": "Node",
		"sourceMap": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"removeComments": false,
		"noImplicitAny": false
	}
}
